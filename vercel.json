{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,

  "routes": [
    { "handle": "filesystem" },
    { "src": "/$",     "dest": "/docs/index.html" },
    { "src": "/(.*)",  "dest": "/docs/$1" }
  ],

  "headers": [
    {
      "source": "/docs/(.*)\\.(png|jpg|jpeg|gif|webp|svg|ico|woff2|ttf|mp4)",
      "headers": [{ "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }]
    }
  ]
}
