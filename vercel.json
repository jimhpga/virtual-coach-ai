{
  "version": 2,
  "routes": [
    { "src": "^/api/(.*)$", "dest": "/api/$1" },
    { "src": "^/(faq|pricing|upload|upload2|report|contact|login|about|analyze|reports?)/?$", "dest": "/public/$1.html" },
    { "src": "^/$", "dest": "/public/index.html" },
    { "src": "^/(.*)$", "dest": "/public/$1" }
  ]
}
