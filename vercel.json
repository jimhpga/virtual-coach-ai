{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/api/:path*", "destination": "/api/:path*" },
    { "source": "/reports/:path*", "destination": "/reports/:path*" },
    { "source": "/upload", "destination": "/docs/upload.html" },
    { "source": "/reports", "destination": "/docs/reports.html" },
    { "source": "/report", "destination": "/docs/report.html" },
    { "source": "/coming-soon", "destination": "/docs/coming-soon.html" },
    { "source": "/pricing", "destination": "/docs/pricing.html" },
    { "source": "/faq", "destination": "/docs/faq.html" },
    { "source": "/contact", "destination": "/docs/contact.html" },
    { "source": "/login", "destination": "/docs/login.html" },
    { "source": "/file-input-test", "destination": "/docs/file-input-test.html" },
    { "source": "/", "destination": "/docs/index.html" },
    { "source": "/:path*", "destination": "/docs/:path*" }
  ],
  "headers": [
    {
      "source": "/(.*)\\.(html|json)",
      "headers": [
        { "key": "Cache-Control", "value": "no-store" }
      ]
    },
    {
      "source": "/(.*)\\.(css|js|png|jpg|jpeg|svg|webp|mp4|mov|m4v|mkv)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ],
  "redirects": [
    { "source": "/index", "destination": "/", "permanent": true },
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "www.virtualcoachai.net" }],
      "destination": "https://virtualcoachai.net/$1",
      "permanent": true
    }
  ],
  "crons": [
    { "path": "/api/process-pending", "schedule": "*/2 * * * *" }
  ]
}
